<div class="card">
    <div class="header">
        <h2>Categories Clouds</h2>
    </div>
    <div class="body widget">
        <ul class="list-unstyled categories-clouds m-b-0">
            <% categories.forEach(function(category){ %>
                <li><a href="/blog/<%= category.label.toLowerCase() %>"><%= category.name %></a></li>
            <% }); %>
        </ul>
    </div>
</div>